#include <stdio.h>

int main() {
  int i;

  printf("Cac so chan tu 1 den 20 la:\n");

  // Sử dụng vòng lặp for để duyệt các số từ 1 đến 20
  for (i = 1; i <= 20; i++) {
    // Kiểm tra xem số hiện tại có phải là số chẵn hay không
    if (i % 2 == 0) {
      printf("%d\t", i); // In số chẵn và một tab
    }
  }

  printf("\n"); // In một dòng mới ở cuối

  return 0;
}
